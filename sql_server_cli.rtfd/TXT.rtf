\fs30 \cf9 \strokec8 \'a0
\fs28 \cf10 \strokec10 '31-90 days'
\fs30 \cf0 \strokec8 \

\fs28 \cf10 \strokec10 \'a0\'a0\'a0\'a0WHEN
\fs30 \cf9 \strokec8 \'a0\'a0
\fs28 DATEDIFF (\cf10 \strokec10 '2020-04-30'\cf9 \strokec8 , date_due) > 90\'a0\cf10 \strokec10 AND
\fs30 \cf9 \strokec8 \'a0
\fs28 DATEDIFF (\cf10 \strokec10 '2020-04-30'\cf9 \strokec8 , date_due) <=180\'a0\cf10 \strokec10 THEN
\fs30 \cf9 \strokec8 \'a0
\fs28 \cf10 \strokec10 '91-180 days'
\fs30 \cf0 \strokec8 \

\fs28 \cf10 \strokec10 \'a0\'a0\'a0\'a0WHEN
\fs30 \cf9 \strokec8 \'a0\'a0
\fs28 DATEDIFF (\cf10 \strokec10 '2020-04-30'\cf9 \strokec8 , date_due) > 180\'a0\cf10 \strokec10 AND
\fs30 \cf9 \strokec8 \'a0
\fs28 DATEDIFF (\cf10 \strokec10 '2020-04-30'\cf9 \strokec8 , date_due) <=365\'a0\cf10 \strokec10 THEN
\fs30 \cf9 \strokec8 \'a0
